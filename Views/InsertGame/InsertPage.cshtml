@model GameModel;

<table>
    <tr>
        <th>Id</th>
        <th>Banner</th>
        <th>Icone</th>
        <th>Nome</th>
        <th>Genero</th>
        <th>Descrição</th>
        <th>Desenvolvedor</th>
        <th>Data de lançamento</th>
    </tr>
</table>

<form method="post" asp-controller="InsertGame" asp-action="CreateGame" id="gameRegister">
    <label>
        <input type="file" asp-for="BannerImgUrl"/>
        <input type="file" asp-for="IconImg"/>
        <input type="text" asp-for="Name" value="@Model.Name" />
        <input type="text" asp-for="Genre" value="@Model.Genre" />
        <input type="text" asp-for="Description" value="@Model.Description" />
        <input type="text" asp-for="Developer" value="@Model.Developer" />
        <input type="datetime" asp-for="RealeseDate" value="@Model.RealeseDate" />
    </label>
</form>
    <button type="submit" form="gameRegister">Enviar</button>